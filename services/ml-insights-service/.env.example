# Port for the ML Insights Service
ML_INSIGHTS_SERVICE_PORT=5001

# Environment (development, staging, production)
NODE_ENV=development

# Logging level (e.g., DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

# URL for Program Service (to fetch base processing times etc.)
PROGRAM_SERVICE_URL=http://program-service:3002/api # Docker Service Name

