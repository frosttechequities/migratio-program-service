{
  "$id": "https://migratio.com/schemas/credentials.json",
  "type": "object",
  "properties": {
    "credentials": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": { "type": "string" },
          "type": { "type": "string" },
          "key": { "type": "string" },
          "secret": { "type": "string" }
        },
        "required": ["id", "type"]
      }
    }
  },
  "required": ["credentials"]
}
